{
  "compilerOptions": {
    "target": "ES2018",
    "module": "CommonJS",
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "sourceMap": true,
    "outDir": "./lib",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "preserveSymlinks": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
